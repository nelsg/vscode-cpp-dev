{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) WSL Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "./build/Debug/bin/cmake_sample_main",
            "cwd": ".",
            "args": [],
            "stopAtEntry": false,
            "environment": [],
            "externalConsole": false,
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "CMake clang++ Debug",
            "windows": {
                "pipeTransport": {
                    "debuggerPath": "/usr/bin/gdb",
                    "pipeProgram": "C:\\Windows\\Sysnative\\bash.exe",
                    "pipeArgs": [
                        "-c"
                    ],
                    "pipeCwd": "${workspaceFolder}"
                },
                "sourceFileMap": {
                    "/mnt/c": "c:\\",
                    "/mnt/d": "d:\\",
                    "/mnt/e": "e:\\",
                    "/mnt/f": "f:\\",
                    "/mnt/g": "g:\\",
                    "/mnt/h": "h:\\",
                    "/mnt/i": "i:\\",
                    "/mnt/j": "j:\\",
                    "/mnt/k": "k:\\",
                    "/mnt/l": "l:\\",
                    "/mnt/m": "m:\\",
                    "/mnt/n": "n:\\",
                    "/mnt/o": "o:\\",
                    "/mnt/p": "p:\\",
                    "/mnt/q": "q:\\",
                    "/mnt/r": "r:\\",
                    "/mnt/s": "s:\\",
                    "/mnt/t": "t:\\",
                    "/mnt/u": "u:\\",
                    "/mnt/v": "v:\\",
                    "/mnt/w": "w:\\",
                    "/mnt/x": "x:\\",
                    "/mnt/y": "y:\\",
                    "/mnt/z": "z:\\"
                }
            },
            "linux": {
                "pipeTransport": {
                    "debuggerPath": "/usr/bin/gdb",
                    "pipeProgram": "/bin/bash",
                    "pipeArgs": [
                        "-c"
                    ],
                    "pipeCwd": "${workspaceFolder}"
                }
            }
        }
    ],
    "compounds": []
}
